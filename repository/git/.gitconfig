[user]
    email = mrogacki@nexway.com
    name = Marcin Rogacki
[color]
    ui = true
[core]
    editor = vim
    filemode = false
    autocrlf = false
	excludesfile = ~/.gitglobal/ignore
[credential]
    helper = cache --timeout=36000
[push]
	default = simple
[alias]
	sync = !~/.gitglobal/subcommand/sync
	hooks = !~/.gitglobal/subcommand/hooks
[diff]
	tool = vimdiff
[merge]
	tool = vimdiff
[url "git@github.com:"]
	insteadOf = https://github.com/
